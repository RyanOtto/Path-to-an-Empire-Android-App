<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryan.PathToAnEmpire">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ryan.PathToAnEmpire.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ryan.PathToAnEmpire.NewGameActivity"></activity>
        <activity android:name="com.example.ryan.PathToAnEmpire.heroPageActivity"></activity>
        <activity android:name="com.example.ryan.PathToAnEmpire.invasionPageActivity"></activity>
        <activity android:name="com.example.ryan.PathToAnEmpire.militaryPageActivity"></activity>
        <activity android:name="com.example.ryan.PathToAnEmpire.religionPageActivity"></activity>
        <activity android:name="com.example.ryan.PathToAnEmpire.tradePageActivity"></activity>

    </application>

</manifest>