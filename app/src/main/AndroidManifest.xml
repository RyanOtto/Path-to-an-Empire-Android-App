<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryan.thesomewhatincrementalgameoflife">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewGameActivity"></activity>
        <activity android:name=".heroPageActivity"></activity>
        <activity android:name=".invasionPageActivity"></activity>
        <activity android:name=".militaryPageActivity"></activity>
        <activity android:name=".religionPageActivity"></activity>
        <activity android:name=".tradePageActivity"></activity>

    </application>

</manifest>